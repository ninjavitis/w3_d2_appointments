<h1>New Patient/h1>
<%= form_with(model: @patient, local:true, class:"ui form") do |f| %>
  <div class="field">
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  </div>
  <div class="field">
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  </div>
  <%= f.submit "Create Patient" %>
<% end %>